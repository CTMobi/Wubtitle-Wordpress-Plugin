{
  "name": "wubtitle",
  "version": "0.1.0",
  "main": "",
  "author": "CTMobi",
  "devDependencies": {
    "@wordpress/plugins": "^2.18.0",
    "@wordpress/scripts": "^11.0.0",
    "csslint": "^1.0.5",
    "eslint": "^7.2.0",
    "husky": "^4.2.5",
    "prettier": "^2.0.5",
    "stylelint-config-wordpress": "^17.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "composer run-script check-php && npm run lint:js:src"
    }
  },
  "scripts": {
    "build": "wp-scripts build",
    "check-engines": "wp-scripts check-engines",
    "check-licenses": "wp-scripts check-licenses",
    "format:js": "wp-scripts format-js",
    "lint:css": "wp-scripts lint-style ./src",
    "lint:js": "wp-scripts lint-js",
    "lint:md:docs": "wp-scripts lint-md-docs",
    "lint:md:js": "wp-scripts lint-md-js",
    "lint:pkg-json": "wp-scripts lint-pkg-json",
    "packages-update": "wp-scripts packages-update",
    "start": "wp-scripts start",
    "test:e2e": "wp-scripts test-e2e",
    "test:unit": "wp-scripts test-unit-js",
    "fixjs": "eslint --fix ./src",
    "lint:js:src": "wp-scripts lint-js ./src",
    "fix:js:src": "wp-scripts lint-js --fix ./src",
    "csslint": "node_modules/.bin/csslint src/ || return 0"
  },
  "dependencies": {
    "@wordpress/api-fetch": "^3.18.0",
    "@wordpress/i18n": "^3.13.0"
  }
}
