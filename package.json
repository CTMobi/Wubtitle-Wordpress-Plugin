{
  "name": "wubtitle",
  "version": "0.1.0",
  "main": "",
  "author": "CTMobi",
  "devDependencies": {
    "@wordpress/plugins": "^2.19.0",
    "@wordpress/scripts": "^12.0.0",
    "csslint": "^1.0.5",
    "eslint": "^7.3.1",
    "husky": "^4.2.5",
    "prettier": "^2.0.5",
    "stylelint-config-wordpress": "^17.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "composer run-script check-php && npm run lint:js:src"
    }
  },
  "scripts": {
    "build-stripe": "wp-scripts build src/stripe/index.js --output-path=build_form --webpack-no-externals",
    "build-data": "cp src/stripe/data/europeanCountries.json build_form/",
    "build": "wp-scripts build && npm run build-stripe && npm run build-data",
    "check-engines": "wp-scripts check-engines",
    "check-licenses": "wp-scripts check-licenses",
    "format:js": "wp-scripts format-js",
    "lint:css": "wp-scripts lint-style ./src",
    "lint:js": "wp-scripts lint-js",
    "lint:md:docs": "wp-scripts lint-md-docs",
    "lint:md:js": "wp-scripts lint-md-js",
    "lint:pkg-json": "wp-scripts lint-pkg-json",
    "packages-update": "wp-scripts packages-update",
    "start": "wp-scripts start",
    "test:e2e": "wp-scripts test-e2e",
    "test:unit": "wp-scripts test-unit-js",
    "fixjs": "eslint --fix ./src",
    "lint:js:src": "wp-scripts lint-js ./src",
    "fix:js:src": "wp-scripts lint-js --fix ./src",
    "csslint": "node_modules/.bin/csslint src/ || return 0"
  },
  "dependencies": {
    "@stripe/react-stripe-js": "^1.1.2",
    "@stripe/stripe-js": "^1.8.0",
    "@wordpress/api-fetch": "^3.18.0",
    "@wordpress/i18n": "^3.14.0",
    "formik": "^2.1.4",
    "yup": "^0.29.1"
  }
}
